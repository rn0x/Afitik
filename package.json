{
  "name": "org.i8xnet.fiti8",
  "private": true,
  "displayName": "Fiti8",
  "version": "1.0.0",
  "description": "Fiti8 is a fitness app offering diverse workouts, balanced nutrition plans, and valuable tips for enhancing health and fitness.",
  "scripts": {
    "keytool": "node keytool.js",
    "dev": "react-scripts start",
    "build": "react-scripts build && mkdir www && xcopy /s /e /y build www && rmdir /s /q build",
    "build-linux": "react-scripts build && mkdir -p www && mv build/* www && rm -r build",
    "device": "cordova run android --device",
    "cordova-build": "cordova build android --release",
    "cordova-android": "cordova platform add android@latest",
    "logcat": "adb logcat",
    "log": "adb logcat | Select-String \"INFO:CONSOLE\"",
    "clean": "cordova clean && npm cache clean --force",
    "rm": "rmdir /s /q node_modules platforms plugins && del package-lock.json && npm run android && npm run device"
  },
  "keywords": [
    "fitness app",
    "health and fitness",
    "workout routines",
    "exercise",
    "diet plans",
    "healthy lifestyle",
    "fitness goals",
    "personal trainer",
    "weight loss",
    "muscle gain",
    "physical fitness",
    "wellbeing",
    "exercise programs",
    "calorie tracker",
    "bodybuilding",
    "healthy eating",
    "wellness app",
    "fitness community"
  ],
  "author": {
    "name": "Ryan Almalki",
    "email": "rn0x.me@gmail.com",
    "url": "https://github.com/rn0x/Fiti8"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "cordova-android": "^13.0.0"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {}
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
